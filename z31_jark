<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>Zad 31 Jarkowicz</title>
</head>

<body>
    <script type="text/javascript">
        var spodnie = {
            producent: "Levi's",
            typ: '501',
            kolor: 'Niebieskie',
            rodzaj: 'Baggy',
            cena: '399zł',
            wyswietl_jarkowicz: function () {
                var output = this.producent + ' ' + this.typ + '<br/>' + this.kolor + '<br/>' + this.rodzaj + '<br/>' + this.cena;
                document.body.innerHTML += output; // Dodaj do treści strony
            }
        }

        spodnie.wyswietl_jarkowicz();

        spodnie.rocznik = 84;
        spodnie.wypisz_jarkowicz = function () {
            alert('Wiek:' + this.rocznik + ' lat')
        }

        spodnie.wypisz_jarkowicz();
    </script>
</body>

</html>
